type pair {
    x: int64,
    y: int64
}

impl pair {
    func copy() ret pair {
        var q = alloc pair;
        q.x = self.x;
        q.y = self.y;
        return q;
    }
}

func foo() ret pair {
    var p = alloc pair;
    p.x = 22;
    return p->copy();
}

func blah() ret int64 {
    let f = foo();
    return f.x;
}

func main() ret int64 {
    assert blah() == 22;
    assert g_FreedObjects > 0;
    return 0;
}

