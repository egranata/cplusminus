func main() ret int64 {
    var a = 0;
    var b = 0;
    var c = 0;
    while (a < 100) {
        while (b < 100) {
            inc c;
            if (c == 10) { continue; };
            inc b;
        };
        if (b == 100) { break; }
        else { inc a; };
    };
    if (a != 0) { return 1; };
    if (b != 100) { return 2; };
    if (c != 101) { return 3; };
    return 0;
}
