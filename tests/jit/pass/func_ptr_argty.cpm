func inc(x: int64) ret int64 {
    return x + 1;
}

func double(x: int64) ret int64 {
    return x + x;
}

type fp = fn(int64) ret int64;

func try(n: int64, f: fp, g: fp) ret int64 {
    return g(f(n));
}

func main() ret int64 {
    var x = 5;
    let a = try(x, inc, double);
    let b = try(x, double, inc);
    return a + b;
}

