ref type wrapper {
    x: int64
}

func foo() ret int64 {
    let p = alloc wrapper;
    p.x = 123;
    let p = p.x;
    let p = p + 1;
    var p = p + 1;
    p = p + 1;
    let p = p + 2;
    return p;
}

func main() ret int32 {
    let n = foo();
    let n = n + g_FreedObjects;
    assert 129 == n;
    assert g_FreedObjects > 0;
    return 0;
}

