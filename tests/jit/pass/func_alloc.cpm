val type foobar {
    x: int32,
    y: int32
}

func alloc_thing() ret foobar {
    return alloc foobar{
        x:3,
        y:3
    };
}

func main() ret int64 {
    let thing = alloc_thing();
    return (thing.x - thing.y) as int64;
}
