ref type ord_pair {
    max: int64,
    min: int64,
    init(x: int64, y: int64) {
        if (x > y) {
            self.max = x;
            self.min = y;
        } else {
            self.max = y;
            self.min = y;
        };
    }
}

func main() ret int64 {
    let p1 = alloc ord_pair;
    let p2 = alloc ord_pair;
    return p1.max + p2.min;
}
