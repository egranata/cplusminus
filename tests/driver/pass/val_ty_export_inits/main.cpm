import "util.bom";

func main() ret int32 {
    let p1 = alloc X(3);
    let p2 = alloc X(2,9);

    assert p1.a == 3;
    assert p1.b == 3;

    assert p2.a == 2;
    assert p2.b == 9;

    return 0;
}
