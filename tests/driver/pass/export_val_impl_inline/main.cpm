import "util.bom";

func foo() {
    let p = alloc Pair{x:12,y:14};
    assert (p.x == 12);
    assert (p.y == 14);
    let p = p->add();
    assert (p == 26);
}

func main() ret int32 {
    foo();
    return 0;
}
